{#def isLast=False, label=False, ui={"separator_class": "double separator"} #}
{% if content %}
{% if label %}
<span class="{% if ui.label_class is defined %}{{ui.label_class}}{% endif %}">
    <strong>
        {{label}}: 
    </strong>
</span>
{% endif%}
<span class="{% if ui.content_class is defined %}{{ui.content_class}}{% endif %}">{{content}}</span>
<Separator isLast={isLast} ui={ui}></Separator>
{% endif %}